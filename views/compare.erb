<header>
	<h1>Results for <%= @first.upcase %> and <%= @second.upcase %></h1>
</header>
<div id="main" role="main">

  <div id="results">
	      <div class="wireline">
          <iframe src="img/network.svg" width="100" height="100" class="svg"></iframe>
          <h3>Wireline Providers</h3>
          <h3><%= @first.capitalize %></h3>
          <table id="wireline_table1" class="tablesorter">
            <thead>
            <tr>
              <th>Provider</th>
              <th>Max Advertised Download Speed</th>
              <th>Max Advertised Upload Speed</th>
            </tr>
          </thead>
          <tbody>
	        <% @wireline1.results.wireline_services.each do |x| %>
            <tr>
              <td><%= x.provider_name %></td>
              <td><%= x.technologies[0].maximum_advertised_download_speed %></td>
              <td><%= x.technologies[0].maximum_advertised_upload_speed %></td>
            </tr>
	        <% end %>
        </tbody>
        </table>
          <h3><%= @second.capitalize %></h3>
          <table id="wireline_table2" class="tablesorter">
            <thead>
            <tr>
              <th>Provider</th>
              <th>Max Advertised Download Speed</th>
              <th>Max Advertised Upload Speed</th>
            </tr>
          </thead>
          <tbody>
            <% @wireline2.results.wireline_services.each do |x| %>
              <tr>
                <td><%= x.provider_name %></td>
                <td><%= x.technologies[0].maximum_advertised_download_speed %></td>
                <td><%= x.technologies[0].maximum_advertised_upload_speed %></td>
              </tr>
             <% end %>
           </tbody>
           </table>
	      </div>
	      <div class="wireless">
          <iframe src="img/wireless.svg" width="100" height="100" class="svg"></iframe>
	         <h3>Wireless Providers</h3>
           <h3><%= @first.capitalize %></h3>
           <table id="wireless_table1" class="tablesorter">
             <thead>
             <tr>
               <th>Provider</th>
               <th>Max Advertised Download Speed</th>
               <th>Max Advertised Upload Speed</th>
             </tr>
           </thead>
           <tbody>
             <% @wireless1.results.wireless_services.each do |x| %>
               <tr>
                 <td><%= x.provider_name %></td>
                 <td><%= x.technologies[0].maximum_advertised_download_speed %></td>
                 <td><%= x.technologies[0].maximum_advertised_upload_speed %></td>
               </tr>
             <% end %>
           </tbody>
           </table>
          <h3><%= @second.capitalize %></h3>
          <table id="wireless_table2" class="tablesorter">
            <thead>
            <tr>
              <th>Provider</th>
              <th>Max Advertised Download Speed</th>
              <th>Max Advertised Upload Speed</th>
            </tr>
          </thead>
          <tbody>
            <% @wireless2.results.wireless_services.each do |x| %>
              <tr>
                <td><%= x.provider_name %></td>
                <td><%= x.technologies[0].maximum_advertised_download_speed %></td>
                <td><%= x.technologies[0].maximum_advertised_upload_speed %></td>
              </tr>
            <% end %>
          </tbody>
          </table>
	      </div>
	    </div>

    </div>
