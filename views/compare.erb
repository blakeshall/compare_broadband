<header>
	<h1>Results for <%= @first.upcase %> and <%= @second.upcase %></h1>
  <a href="http://github.com/you"><img style="position: absolute; top: 0; right: 0; border: 0;" src="https://a248.e.akamai.net/assets.github.com/img/71eeaab9d563c2b3c590319b398dd35683265e85/687474703a2f2f73332e616d617a6f6e6177732e636f6d2f6769746875622f726962626f6e732f666f726b6d655f72696768745f677261795f3664366436642e706e67" alt="Fork me on GitHub"></a>
</header>
<div id="main" role="main">

  <div id="results">
	  <div id="left">
	    <h3><%= @first.capitalize %></h3>
	      <div class="wireline">
          <iframe src="img/network.svg" width="100" height="100" class="svg"></iframe>
	        <h4>Wireline Providers</h4>
	        <% @wireline1.results.wireline_services.each do |x| %>
	          <h5><%= x.provider_name %></h5>
	          <p>Max Advertised Download Speed: <span><%= x.technologies[0].maximum_advertised_download_speed %></span>Mbs</p>
	          <p>Max Advertised Upload Speed: <span><%= x.technologies[0].maximum_advertised_upload_speed %></span>Mbs</p>
	        <% end %>
	      </div>
	      <div class="wireless">
          <iframe src="img/wireless.svg" width="100" height="100" class="svg"></iframe>
	         <h4>Wireless Providers</h4>
	          <% @wireless1.results.wireless_services.each do |x| %>
	          <h5><%= x.provider_name %></h5>
	          <p>Max Advertised Download Speed: <span><%= x.technologies[0].maximum_advertised_download_speed %></span>Mbs</p>
	          <p>Max Advertised Upload Speed: <span><%= x.technologies[0].maximum_advertised_upload_speed %></span>Mbs</p>
	        <% end %>
	      </div>
	    </div>
	    <div id = "right">
	      <h3><%= @second.capitalize %></h3>
	      <div class="wireline">
          <iframe src="img/network.svg" width="100" height="100" class="svg"></iframe>
	        <h4>Wireline Providers</h4>
	       <% @wireline2.results.wireline_services.each do |x| %>
	         <h5><%= x.provider_name %></h5>
	         <p>Max Advertised Speed: <span><%= x.technologies[0].maximum_advertised_download_speed %></span>Mbs</p>
	         <p>Max Advertised Upload Speed: <span><%= x.technologies[0].maximum_advertised_upload_speed %></span>Mbs</p>
	       <% end %>
	     </div>
	     <div class="wireless">
         <iframe src="img/wireless.svg" width="100" height="100" class="svg"></iframe>
	        <h4>Wireless Providers</h4>
	        <% @wireless2.results.wireless_services.each do |x| %>
	          <h5><%= x.provider_name %></h5>
	          <p>Max Advertised Download Speed: <span><%= x.technologies[0].maximum_advertised_download_speed %></span>Mbs</p>
	          <p>Max Advertised Upload Speed: <span><%= x.technologies[0].maximum_advertised_upload_speed %></span>Mbs</p>
	        <% end %>
	      </div>
	     </div>
    </div>
